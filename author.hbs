{{!< default}}

<div class="content-area">
    <main class="site-main">
        {{#author}}
            <div class="term-author kg-width-normal">
                {{> author}}
            </div>
        {{/author}}
        <div class="post-feed kg-width-normal">
            {{#foreach posts}}
                {{> "loop"}}
            {{/foreach}}
        </div>
        {{pagination}}
    </main>
</div>